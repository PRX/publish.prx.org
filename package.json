    "core-js": "^2.4.1",
    "dotenv": "^2.0.0",
    "evaporate": "^1.5.2",
    "http-server": "^0.9.0",
    "ng2-dragula": "^1.2.0",
    "rxjs": "5.0.0-beta.12",
